{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-mal": "esbuild ./src/bot/malBot.ts --bundle --minify --sourcemap --platform=node --target=es2021 --outfile=dist/malBot.js",
    "build-db": "esbuild ./src/bot/dbBot.ts --bundle --minify --sourcemap --platform=node --target=es2021 --outfile=dist/dbBot.js",
    "build-acg": "esbuild ./src/bot/acgBot.ts --bundle --minify --sourcemap --platform=node --target=es2021 --outfile=dist/acgBot.js",
    "build-image": "esbuild ./src/bot/imageBot.ts --bundle --minify --sourcemap --platform=node --target=es2021 --external:sharp --outfile=dist/imageBot.js",
    "test-mal": "ts-node -r dotenv/config src/bot/malBot.ts dotenv_config_path=./.env.local",
    "test-db": "ts-node -r dotenv/config src/bot/dbBot.ts dotenv_config_path=./.env.local",
    "test-acg": "ts-node -r dotenv/config src/bot/acgBot.ts dotenv_config_path=./.env.local",
    "test-image": "ts-node -r dotenv/config src/bot/imageBot.ts dotenv_config_path=./.env.local"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.53.0",
    "@aws-sdk/client-s3": "^3.88.0",
    "@aws-sdk/client-ssm": "^3.81.0",
    "@aws-sdk/lib-dynamodb": "^3.53.0",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mdx-js/loader": "^2.0.0",
    "@next-auth/dynamodb-adapter": "^1.1.0",
    "@next/mdx": "^12.1.0",
    "axios": "^0.26.0",
    "cheerio": "^1.0.0-rc.12",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.2",
    "framer-motion": "^6.2.8",
    "nanoid": "^3.3.1",
    "next": "^12.1.0",
    "next-auth": "^4.2.1",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-query": "^3.34.16",
    "react-use": "^17.3.2",
    "sharp": "^0.30.7",
    "web-push": "^3.4.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.106",
    "@types/mdx-js__react": "^1.5.5",
    "@types/ramda": "^0.28.1",
    "@types/react": "17.0.39",
    "@types/sharp": "^0.31.0",
    "@types/web-push": "^3.3.2",
    "esbuild": "^0.15.9",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "4.6.2"
  }
}
